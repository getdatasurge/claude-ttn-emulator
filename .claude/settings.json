  "statusLine": {
    "type": "command",
    "command": "input=$(cat); printf \"\\033[32m$(whoami)@$(hostname -s)\\033[0m:\\033[34m$(basename \"$(echo \"$input\" | jq -r '.workspace.current_dir')\")\\033[0m [\\033[35m$(echo \"$input\" | jq -r '.model.display_name' | cut -d' ' -f1-2)\\033[0m]\\033[0m\""
